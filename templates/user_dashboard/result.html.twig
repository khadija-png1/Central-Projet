{% if projets is not empty %}
<div class="d-flex justify-content-center">
    <table class="table table-striped table-bordered">    
    <thead>
        <tr>
            <th>Nom</th>     
        </tr>
    </thead>
    <tbody>
        {% for projet in projets %}
            <tr>
                <td>{{ projet.nom }}</td>
                
            </tr>
        {% endfor %}
    </tbody>
</table>
</div>
{% else %}
    <div class="alert alert-warning">Aucun projet trouv√© .</div>
{% endif %}
